<%= form_for @employee do |f| %>
<%= f.label :first_name %> <br>
<%= f.text_field :first_name %><br><br>

<%= f.label :last_name %><br>
<%= f.text_field :last_name %><br><br>

<%= f.label :alias %><br>
<%= f.text_field :alias %><br><br>

<%= f.label :title %>
<%= f.text_field :title %> <br><br>

<%= f.label :office %>
<%= f.text_field :office %><br><br>

<%= f.label "Image" %><br>
<%= f.text_field :img_url %><br>
<%= f.collection_radio_buttons :dog_id, @dogs, :id, :name %>

<%= f.submit %>
<% end %>
